int main()
{
    int base, altura, area;
    
    printf("Qual a área da base do retângulo?: ");
    scanf("%d",&base); 
    
    printf("Qual a área da altura do retângulo?: ");
    scanf("%d",&altura);
    
    area = base * altura;
    
    printf("o valor da area do retângulo é igual a %d",area);
    return 0;
}
