#include <stdio.h>

int main()
{
    float carros, vendasTotais, salario, valorCarro, valorFinal;
    
    printf("informe quantos carros foram vendidos: ");
    scanf ("%f",&carros);
    
    printf("\nIforme o valor total de seu lucro: ");
    scanf ("%f",&vendasTotais);
    
    printf ("\nIforme o salario Fixo: ");
    scanf("%f",&salario);
   
    printf("\nInforme o valor recebido de cada carro vendido: ");
    scanf("%f" ,&valorCarro);
   
    valorFinal = salario + valorCarro + vendasTotais *0.05;
    
    printf("O valor total do salario do Weberti Ã© R$%.2f", valorFinal);
    
    
    return 0;
}
